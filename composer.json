{
    "name": "findbrok/php-watson-api-bridge",
    "description": "A simple PHP Wrapper around IBM Watson API",
    "keywords": ["php", "ibm", "watson", "api", "bridge"],
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "6.2.*",
        "illuminate/support": "5.0 - 5.3",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*",
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "FindBrok\\WatsonBridge\\" : "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
